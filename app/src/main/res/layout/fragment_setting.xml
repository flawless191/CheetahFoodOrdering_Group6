        app:layout_constraintStart_toStartOf="@+id/imageView45"
        app:layout_constraintTop_toTopOf="@+id/imageView45" />

    <ImageView
        android:id="@+id/imageView44"
        android:layout_width="24dp"
        android:layout_height="29dp"
        android:src="@drawable/material_symbols_chevron_right"
        app:layout_constraintEnd_toEndOf="@+id/imageView45"
        app:layout_constraintTop_toTopOf="@+id/imageView45" />

    <ImageView
        android:id="@+id/imageView43"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginStart="20dp"
        android:layout_marginTop="30dp"
        android:src="@drawable/rectangle_27"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/textView18" />

    <TextView
        android:id="@+id/textView28"
        android:layout_width="318dp"
        android:layout_height="29dp"
        android:text="Question"
        android:textColor="#726C6C"
        android:textSize="18dp"
        app:layout_constraintEnd_toEndOf="@+id/imageView43"
        app:layout_constraintTop_toTopOf="@+id/imageView43" />

    <ImageView
        android:id="@+id/imageView40"
        android:layout_width="24dp"
        android:layout_height="29dp"
        android:src="@drawable/material_symbols_chevron_right"
